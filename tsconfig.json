{
  "ts-node": {
    // these options are overrides used only by ts-node
    // same as the --compilerOptions flag and the TS_NODE_COMPILER_OPTIONS environment variable
    "compilerOptions": {
      "module": "NodeNext",
    },
    "include": [
      "./npm_colab"
    ]
  },
  "compilerOptions": {
    "outDir": "./dist",
    "strict": true,
    "sourceMap": true,
    "rootDir": "./src",
  },
  "include": [
    "src/**/*.tsx",
    "src/**/*.jsx",
    "src/**/*.ts"
  ],
}